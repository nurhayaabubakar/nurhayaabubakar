#include <iostream>
using namespace std;

void tambah (int *angka) {
*angka +=20;
}

int main () {
int nilai = 10;
cout<<"Nilai variabel nilai adalah ="<<nilai<<endl;
tambah (&nilai); // Memasukan alamat  variabel nilai pada fungsi tambah 
cout<<"nilai variabel nilai adalah ="<<nilai<<endl;
return 0;
}
